<%- include('includes/head.ejs') %>
<body>
  <main>
    <% if (users.length > 0) { %>
      <h3>List of users</h3>
      <ul>
        <% for (let user of users) { %>
          <li><%= user.name %></li>
        <% } %>
      </ul>
    <% } else { %>
      <h1>No users added!</h1>
    <% } %>
  </main>
</body>
</html>